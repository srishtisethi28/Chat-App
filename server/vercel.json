{
    "version":2,
    "builds":[
        {

            "src":"serveer.js",
            "use":"@vercel/node",
            "config":{
                "includeFiles":[
                    "dist/**"
                ]
            }
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"server.js"
        }
    ]
}